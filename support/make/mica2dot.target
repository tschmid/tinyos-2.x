PLATFORM = mica2dot

PROGRAMMER ?= uisp
ifeq ($(PROGRAMMER),avrdude)
  PROGRAMMER_PART ?= -pm128  -U efuse:w:0xff:m
endif

ifeq ($(PROGRAMMER),uisp)
  PROGRAMMER_PART ?= -dpart=ATmega128  --wr_fuse_e=ff
endif

AVR_FUSE_H ?= 0xd9

$(call TOSMake_include_platform,avr)

mica2dot: $(BUILD_DEPS)
	@:

